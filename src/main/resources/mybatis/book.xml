<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="queries">

    <select id="selectBooks" resultType="myweb.web1.model.Book" parameterType="int">
        SELECT bookId, title  from book WHERE bookId > #{id}
    </select>
</mapper> 